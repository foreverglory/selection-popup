    "methed": "json",
    "template": "<h3>{{result.word_name}}</h3><p v-if='result.exchange'>变形：<span v-for='word in result.exchange'>{{word}}</span></p><p v-for='item in result.symbols[0].parts'><em>{{item.part}}</em> <span v-for='mean in item.means'>{{mean}}; </span></p>",
